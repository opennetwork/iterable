{
  "name": "iterable",
  "version": "5.2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc --declaration && npm run tslint && node correct-import-extensions.js",
    "watch": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "test": "yarn build && node dist/tests/index.js",
    "example:server": "yarn build && node examples/node-server.js",
    "example:view": "yarn build && node examples/view.js",
    "coverage": "c8 -r html -r text-summary yarn test"
  },
  "peerDependencies": {},
  "dependencies": {
    "@opennetwork/linked-list": "^5.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opennetwork/iterable.git"
  },
  "devDependencies": {
    "@betterer/cli": "^0.1.3",
    "@types/node": "^16.11.12",
    "c8": "^7.10.0",
    "esm": "^3.2.25",
    "filehound": "^1.17.5",
    "tslint": "^6.1.3",
    "typescript": "^4.5.3"
  }
}
