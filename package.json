{
  "name": "iterable",
  "version": "6.0.1-beta.7",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "polyfill": [
        "./dist/polyfill/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./polyfill": "./dist/polyfill.js"
  },
  "scripts": {
    "build": "tsc --declaration && node correct-import-extensions.js",
    "watch": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "test": "yarn build && node --enable-source-maps dist/tests/index.js",
    "example:server": "yarn build && node examples/node-server.js",
    "example:view": "yarn build && node examples/view.js",
    "coverage": "c8 yarn test"
  },
  "peerDependencies": {},
  "dependencies": {
    "@opennetwork/linked-list": "^5.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opennetwork/iterable.git"
  },
  "devDependencies": {
    "@betterer/cli": "^0.1.3",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.4.5",
    "@virtualstate/promise": "^1.3.4",
    "@virtualstate/union": "^2.48.1",
    "c8": "^7.10.0",
    "esm": "^3.2.25",
    "filehound": "^1.17.5",
    "jsdom": "^22.1.0",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6"
  }
}
